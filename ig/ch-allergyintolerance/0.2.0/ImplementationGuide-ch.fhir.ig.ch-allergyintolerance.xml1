      </reference>
      <name value="CH Allergy Intolerance Composition Profile"/>
      <description value="Definition of the composition for the allergy intolerance document"/>
      <exampleBoolean value="false"/>
    </resource>
    <resource>
      <extension url="http://hl7.org/fhir/tools/StructureDefinition/resource-information">
